@model Student
<html>
<head>
    <title></title>
</head>
<body>
    <form class="Form-horizantal" asp-controller="Home" asp-action="Update" method="post">

        <div class="form-group">
            <lable class="control-lable- col-sm-2" asp-for="EmailId">Student EmailId</lable>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="EmailId" />
                <span asp-validation-for="EmailId" class="text-denger"></span>
            </div>
        </div>
        <br />


        <div class="form-group">
            <lable class="control-lable- col-sm-2" asp-for="FirstName">Student FirstName :</lable>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="FirstName" />
                <span asp-validation-for="FirstName" class="text-denger"></span>
            </div>
        </div><br />

        <div class="form-group">
            <lable class="control-lable- col-sm-2" asp-for="LastName">Student LastName :</lable>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="LastName" />
                <span asp-validation-for="LastName" class="text-denger"></span>
            </div>
        </div><br />

        



        <div class="form-group">
            <lable class="control-lable- col-sm-2" asp-for="Gender">Student Gender :</lable>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="Gender" />
                <span asp-validation-for="Gender" class="text-denger"></span>
            </div>
        </div><br />

        <div class="form-group">
            <lable class="control-lable- col-sm-2" asp-for="MobileNumber">Student MobileNumber :</lable>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="MobileNumber" />
                <span asp-validation-for="MobileNumber" class="text-denger"></span>
            </div>
        </div>
        <br />
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" value="Update" class="btn-primary">Update</button>

            </div>

        </div>

    </form>

</body>
</html>
